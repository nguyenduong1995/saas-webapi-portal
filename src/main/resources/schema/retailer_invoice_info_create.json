{
  "$id": "https://consumerapi.ipisaas.com/schema/retailer_invoice_info_create.json",
  "$schema": "https://json-schema.org/draft/2019-09/schema#",
  "title": "Rules",
  "type": "object",
  "required": [
    "retailer_id",
    "tel"
  ],
  "properties": {
    "retailer_id": {
      "type": "integer",
      "description": "id retailer",
      "minimum": 0
    },
    "tel": {
      "type": "string",
      "description": "Telephone",
      "pattern": "^(03|07|08|09|01[2|6|8|9])+([0-9]{8})$"
    }
  },
  "additionalProperties": true
}